{
  "version": 2,
  "name": "bird-view",
  "env": {
    "GRAPHQL_SERVER_URI": "@birdview-graphql-server-uri"
  },
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "routes": [
